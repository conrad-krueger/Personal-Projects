        "data.pop(\"20\")\n",
        "test.pop(\"21\")\n",
        "\n",
        "#get ride of \"duration\" column\n",
        "data.drop(\"0\",axis=1,inplace=True)\n",
        "test.drop(\"0\",axis=1,inplace=True)\n",
        "\n",
        "#Preproccessing\n",
        "#eliminating columns filled with nulls\n",
        "for col in data:\n",
        "  if (pd.isna(data[col].loc[0])):\n",
        "    data.drop([col],axis=1,inplace=True)\n",
        "\n",
        "#Renaming columns \n",
        "data.columns = all\n",
