        "'''\n",
        "\n",
        "\n",
        "#Or...\n",
        "#Convert to numerical values\n",
        "\n",
        "reference_for_pred = []\n",
        "for name in [\"service\", \"protocol_type\",\"flag\"]:\n",
        "  ref = list(data[name].unique())\n",
        "  reference_for_pred.append(ref)\n",
        "  convert = []\n",
        "  for x in range(len(data[name])):\n",
        "    convert.append(ref.index(data[name][x])/len(ref))\n",
        "\n",
        "  data.drop([name],axis=1,inplace=True)\n",
        "  data.insert(0,name,convert,True)\n",
        "\n",
        "for name in [\"service\", \"protocol_type\",\"flag\"]:\n",
        "  ref = list(test[name].unique())\n",
        "  convert = []\n",
        "  for x in range(len(test[name])):\n",
