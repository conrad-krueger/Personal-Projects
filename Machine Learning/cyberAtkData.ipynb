      "outputs": [],
      "source": [
        "model = keras.Sequential([ #sequential is just data going from left to right\n",
        "    keras.layers.Flatten(input_shape=(40, )),  # input layer 40 nodes to start one for each column still present\n",
        "    keras.layers.Dense(256, activation='relu'), \n",
        "    keras.layers.Dense(256, activation='relu'), \n",
        "    keras.layers.Dense(64, activation='relu'), \n",
        "    keras.layers.Dense(23, activation='softmax') # output layer (3) softmax make sures vals are btwn 0 and 1 and all vals add up to 1\n",
        "])\n",
        "\n"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
