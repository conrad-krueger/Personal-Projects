      "source": [
        "##COMPILE MODEL##\n",
        "model.compile(optimizer=keras.optimizers.Adam(\n",
        "    learning_rate=0.005), #bc no other optimizer beside Adam exists in my eyes \n",
        "              loss='sparse_categorical_crossentropy', #categorizing type of attk\n",
        "              metrics=['accuracy']) #want to see accuracy of model after each epoch"
